---
import {Image} from "astro:assets"




interface Props {
path: string;
width: number ;
height: number;
alt: string;
}

const {path, width, alt , height, } = Astro.props
---

<Image src={path} width={width} alt={alt} height={height}  class="w-full h-80 hover:scale-110 transition-all ease-in delay-300 cursor-pointer object-center object-cover"/>

